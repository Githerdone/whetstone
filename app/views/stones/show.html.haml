- content_for :right_tabs do
  = link_to "Overview", stone_overview_path(@stone) , remote: true, method: 'get', class: "get_overview"
  = link_to "Resources", stone_resources_path(@stone), remote: true,  class: "get_resources"
  = link_to "Discussions", stone_discussions_path(@stone), remote: true, method: "get", class: "get_discussions"
= render partial: 'overview', locals: {stone: @stone, goal: @goal}
